        ],
        "keyphrases":[
            {"eus": "(Nik) egiten dut", "spa": "(Yo) [lo,la] hago"},
            {"eus": "(Zuk) egiten duzu", "spa": "(Tú) [lo,la] haces"},
            {"eus": "(Hark) egiten du", "spa": "(Él,ella) [lo,la] hace"},
            {"eus": "(Guk) egiten dugu", "spa": "(Nosotros,Nosotras) [lo,la] hacemos"},
            {"eus": "(Zuek) egiten duzue", "spa": "(Vosotros,Vosotras) [lo,la] hacéis"},
            {"eus": "(Haiek) egiten dute", "spa": "(Ellos,Ellas) [lo,la] hacen"},
            {"eus": "(Nik) euskara ikasi dut", "spa": "(Yo) he estudiado euskera"},
            {"eus": "(Hura) futbolean jokatzen du", "spa": "(Él,ella) juega (al) fútbol"},
            {"eus": "(Nik) beti zortzi orduz lan <egiten> dut", "spa": "(Yo) siempre trabajo ocho horas"},
            {"eus": "(Hark) futbolean <jokatzen> du, ez saskibaloian", "spa": "(Él,Ella) juega al fútbol, no al baloncesto"},
            {"eus": "Guk hemen <jokatzen> dugu eta zuek hor jokatzen duzue", "spa": "Nosotros jugamos aquí y vosotros jugáis ahí"},
            {"eus": "(Haiek) hotelean lo egiten dute orain", "spa": "Ellos duermen ahora en el hotel"},
